.a-button-group {
  position: relative;
  display: flex; }

.a-button-group--block {
  border-radius: 4px; }
  .a-button-group--block .a-button {
    border-radius: 0 !important; }

.a-button {
  position: relative;
  font-size: 14px;
  vertical-align: middle;
  display: inline-block;
  margin: 0;
  text-align: center;
  padding: 0 8px;
  min-height: 44px;
  min-width: 44px;
  box-sizing: border-box;
  border: 0 none;
  user-select: none;
  outline: none;
  border-radius: 4px;
  background: #fff;
  color: #000;
  line-height: 1;
  transition: box-shadow 200ms;
  border-width: 1px;
  border-style: solid; }
  .a-button::after {
    content: ""; }
  .a-button + .a-button {
    margin-left: 8px; }
  .a-button--block, .a-button--full {
    font-size: 16px;
    width: 100%;
    padding: 16px;
    border-radius: 4px; }
  .a-button--full {
    border-radius: 0 !important; }
  .a-button--rounded {
    border-radius: 99999px; }
  .a-button--round {
    border-radius: 99999px; }
  .a-button--loading::before {
    display: inline-block;
    height: 1em;
    width: 1em;
    border-radius: 1em;
    border-style: solid;
    border-width: 0.1em;
    border-color: transparent currentColor transparent transparent;
    animation: rotate 0.6s linear infinite;
    vertical-align: middle;
    margin-right: 8px;
    content: "";
    height: 0.6em;
    width: 0.6em; }

